/* packageExhibitor/pages/zEdition1/purchaser/detail/index.wxss */
@import "../../components/common.wxss";
.content{
}
.label1{
  display: inline-block;
  font-size:32rpx;
  font-family:Microsoft YaHei;
  font-weight:400;
  color:rgba(51,51,51,1);
  /* width: 210rpx; */
  text-align: center;
  border-bottom: 4rpx solid #FF2D2E;
  margin-top: 67rpx;
  margin-left: 31rpx;
}
.space-line{
  width: 100%;
  height: 1rpx;
  background-color: #DCDFE6;
}
.item-wrap{
  background-color: white;
  padding: 30rpx 50rpx;
  padding-right: 30rpx;
}
.header{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.header>.title{
  font-size:26rpx;
  font-family:Microsoft YaHei;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.header>.eay-wrap{
  flex-shrink: 0;
  margin-left: 30rpx;
}
.header>.eay-wrap>.eay{
  width: 30rpx;
  height: 22rpx;
  margin-right: 20rpx;
}
.header>.eay-wrap>text{
  font-size:28rpx;
  font-family:Microsoft YaHei;
  font-weight:400;
  color:rgba(255,45,48,1);
}
.body{
  margin-top: 29rpx;
  display: flex;
  flex-direction: column;
}
.body>text{
  font-size:24rpx;
  font-family:Microsoft YaHei;
  font-weight:400;
  color:rgba(102,102,102,1);
  margin-bottom: 18rpx;
}
.big-space{
  width: 100%;
  height: 20rpx;
  background-color: #F8F8F8;
}
.middle-label{
  border: 1px solid #FF2D2E;
  font-size:22rpx;
  font-family:Microsoft YaHei;
  font-weight:400;
  color:rgba(255,46,46,1);
  margin-right: 20rpx;
  display: inline-block;
  padding-left: 11rpx;
  padding-right: 11rpx;
  border-radius:6rpx;
}