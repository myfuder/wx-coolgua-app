.search-wrap{
  padding: 30rpx;
}
.search-input{
  width:560rpx;
  height:68rpx;
  background:rgba(255,255,255,1);
  border:1px solid rgba(235,238,245,1);
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-left: 14rpx;
}
.search-input>input{
  font-size:26rpx;
  font-family:Microsoft YaHei;
  font-weight:400;
  color:rgba(153,153,153,1);
  margin-left: 27rpx;
}
.search-icon{
  width: 33rpx;
  height: 33rpx;
}
.header{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.filter-text{
  font-size:28rpx;
  font-family:Microsoft YaHei;
  font-weight:400;
  color:rgba(102,102,102,1);
  margin-left: 14rpx;
}
.switch-down{
  width: 26rpx;
  height: 14rpx;
  background-color: green;
  margin-left: 11rpx;
}
.filter-wrap{
  padding-left: 18rpx;
  padding-right: 18rpx;
}
.child{
  padding-left: 40rpx;
}
.item-wrap{
  margin-bottom: 20rpx;
}
.father-wrap .checkbox-style{
  font-family:Microsoft YaHei;
  font-weight:400;
  font-size: 33rpx;
  color:rgba(51,51,51,1);
  /* transform: scale(0.8,0.8); */
}
.child .checkbox-style{
  font-family:Microsoft YaHei;
  font-weight:400;
  font-size: 31rpx;
  color:rgba(102,102,102,1);
  margin-right: 20rpx;
  /* transform: scale(0.8,0.8); */
}
.loading{
  display: block;
  text-align: center;
  padding-top: 20rpx;
  padding-bottom: 10rpx;
}
.page-wrap{
  display: flex;
  flex-direction: row;
  padding: 30rpx 118rpx;
  justify-content: space-between;
  align-items: center;
}
.page-wrap>text:first-of-type, .page-wrap>text:last-of-type{
    display: block;
    width: 140rpx;
    height: 60rpx;
    line-height: 60rpx;
    border: 1px solid rgba(216,218,222,1);
    border-radius:3rpx;
    font-size:26rpx;
    color:rgba(67,67,67,1);
    text-align: center;
}
.page-wrap>text:nth-child(2) {
  font-size:26rpx;
  font-family:Microsoft YaHei;
  font-weight:400;
  color:rgba(102,102,102,1);
}
.page-wrap .activeClass{
  border:1px solid rgba(253,70,106,1) !important;
  color:rgba(254,68,101,1) !important;
}
.row-container{
    margin-top: 20rpx;
    padding-left: 20rpx;
    padding-right: 20rpx;
}