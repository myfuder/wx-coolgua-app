/**register.wxss**/
.clearfix:after{
  content:".";
  display:block;
  height:0;
  clear:both;/*清除浮动*/
  visibility:hidden;
}
.clearfix{zoom:1;/*为IE6，7的兼容性设置*/}
page{
  width:100%;
  height:100%;
}
/**绑定展商页**/
.container{
  width:100%;
  height:100%;
  position:relative;
  background:#f7f7f7;
}
/**表单内容**/
.container form{
  width:100%;
  height:100%;
  position:relative;
  padding:20rpx 0 0 0;
}
.container .form-sign{
  width:100%;
}
.container .form-sign .form-item{
  padding:15rpx 40rpx;
}
.container .form-sign .form-item .name{
  font-size:26rpx;
  color:#333333;
  margin-bottom:15rpx;
}
.container .form-sign .form-item .name text{ 
  color:#ff5400;
}
.container .form-sign .form-item input{ 
  background:#fff;
  border-radius:5rpx;
  font-size:26rpx;
  padding-left:20rpx;
}
.notice{
  width:100%;
  margin: 80rpx auto 0;
  text-align:center;
  font-size:26rpx;
  color:#cccccc;
}
.notice .telephone{
  font-size: 24rpx;
  color: #003990;
  margin-top:10rpx;
  display:inline-block;
}
.container .formTitle{
  padding:0 40rpx;
  margin-top:120rpx;
}
.container .formTitle .tab-ticket{
  font-size: 32rpx;
  color:#fff;
  border-radius: 10rpx;
  width: 90%;
  height: 90rpx;
  line-height: 90rpx;
  margin:0 auto; 
  border:none;
  text-align:center; 
  background: #ff0000;
}
/**展商版注册按钮**/
.container .register{
  width:100%;
  margin:80rpx auto 20rpx;
  text-align:center;
}
.container .register .button{
  font-size: 32rpx;
  color:#fff;
  border-radius: 10rpx;
  width: 80%;
  height: 90rpx;
  line-height: 90rpx;
  margin:0 auto; 
  border:none;
  text-align:center;
}
.container .register .exhibitor{
  background:#fff;
  color: #666;
  border-color: #666;
  margin-top:70rpx;
}